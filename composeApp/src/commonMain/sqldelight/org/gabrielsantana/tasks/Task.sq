CREATE TABLE Task (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    description TEXT NOT NULL,
    isChecked INTEGER NOT NULL
);

insert:
INSERT OR REPLACE INTO Task(id, description, isChecked)
VALUES(?,?,?);

getAll:
SELECT * FROM Task;

